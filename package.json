    "docxyz": "^0.2.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
    "multer": "^1.4.5-lts.1",
    "openai": "^3.1.0"
  },

  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
